pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/mariamosamaa/ls_script_repo.git'
            }
        }
        stage('Execute Script') {
            steps {

                // Execute the script
                bat 'ls.sh'
               script {
                    def exitCode = sh returnStatus: true, script: './ls.sh'
                    if (exitCode == 0) {
                        echo 'Script executed successfully.'
                    } else {
                        error 'Script execution failed.'
                    }
                }
            }
        }
    }
}


