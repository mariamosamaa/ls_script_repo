pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/mariamosamaa/ls_script_repo.git'
            }
        }
        stage('Execute Script') {
            steps {
                // Ensure the script is executable (if needed, though it's not required on Windows)
                sh 'chmod +x ls.sh'
                // Execute the script
                sh './ls.sh'
            }
        }
    }
}


